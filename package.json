{
  "name": "posthtml-inline-css",
  "version": "0.0.0",
  "description": "CSS inliner",
  "main": "index.js",
  "author": "Kirill Maltsev <maltsevkirill@gmail.com>",
  "license": "MIT",
  "scripts": {
    "compile": "rm -f lib/*.js && node_modules/.bin/babel -d lib/ lib/",
    "lint": "node_modules/.bin/eslint *.js lib/*.es6 test/",
    "pretest": "npm run lint",
    "test": "node_modules/.bin/_mocha --compilers js:babel-core/register"
  },
  "keywords": [
    "posthtml",
    "posthtml-plugin",
    "html",
    "css",
    "inliner",
    "inline",
    "style"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "postcss": "^5.0.12",
    "posthtml-match-helper": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.5",
    "babel-polyfill": "^6.1.19",
    "babel-preset-es2015": "^6.1.18",
    "eslint": "^1.9.0",
    "expect": "^1.13.0",
    "lodash": "^3.10.1",
    "mocha": "^2.3.4",
    "posthtml": "^0.7.0"
  }
}
